generator client {
  provider = "prisma-client-js"
  output   = "../Service_db_prisma/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("SERVICE_DATABASE_URL")
}

model User {
  id        Int       @id
  firstname String
  lastname  String
  email     String    @unique
  password  String
  tenants   Tenants[]
}

model Tenants {
  id   Int    @id
  user User[]
  name String
  url  String
}
